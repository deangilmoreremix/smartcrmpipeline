# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_MODEL=gpt-4o

# Google Gemini AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Available Gemini/Gemma Models:
# - gemini-2.0-flash-exp (Latest Gemini 2.0 Flash)
# - gemini-1.5-flash (Gemini 1.5 Flash)  
# - gemini-1.5-pro (Gemini 1.5 Pro)
# - gemma-2-2b-it (Gemma 2 2B Instruction Tuned)
# - gemma-2-9b-it (Gemma 2 9B Instruction Tuned)
# - gemma-2-27b-it (Gemma 2 27B Instruction Tuned)
VITE_GEMINI_MODEL=gemini-2.0-flash-exp

# Supabase Configuration (for database)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Email Service (for AI-generated emails)
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_FROM_EMAIL=noreply@yourcrm.com

# External CRM Integration (optional)
VITE_CRM_API_URL=your_crm_api_url
VITE_CRM_API_KEY=your_crm_api_key